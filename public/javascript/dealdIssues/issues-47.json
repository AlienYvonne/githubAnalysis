[{"url": "https://api.github.com/repos/airbnb/javascript/issues/358", "repository_url": "https://api.github.com/repos/airbnb/javascript", "labels_url": "https://api.github.com/repos/airbnb/javascript/issues/358/labels{/name}", "comments_url": "https://api.github.com/repos/airbnb/javascript/issues/358/comments", "events_url": "https://api.github.com/repos/airbnb/javascript/issues/358/events", "html_url": "https://github.com/airbnb/javascript/issues/358", "id": 85905000, "node_id": "MDU6SXNzdWU4NTkwNTAwMA==", "number": 358, "title": "componentDidUpdate is missing in react style guide?", "user": {"login": "ycavatars", "id": 1477617, "node_id": "MDQ6VXNlcjE0Nzc2MTc=", "avatar_url": "https://avatars2.githubusercontent.com/u/1477617?v=4", "gravatar_id": "", "url": "https://api.github.com/users/ycavatars", "html_url": "https://github.com/ycavatars", "followers_url": "https://api.github.com/users/ycavatars/followers", "following_url": "https://api.github.com/users/ycavatars/following{/other_user}", "gists_url": "https://api.github.com/users/ycavatars/gists{/gist_id}", "starred_url": "https://api.github.com/users/ycavatars/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/ycavatars/subscriptions", "organizations_url": "https://api.github.com/users/ycavatars/orgs", "repos_url": "https://api.github.com/users/ycavatars/repos", "events_url": "https://api.github.com/users/ycavatars/events{/privacy}", "received_events_url": "https://api.github.com/users/ycavatars/received_events", "type": "User", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 3, "created_at": "2015-06-07T14:02:35Z", "updated_at": "2015-06-19T15:25:29Z", "closed_at": "2015-06-19T15:25:29Z", "author_association": "CONTRIBUTOR", "body": "The react style guide doesn't contain `componentWillUpdate`. Should it be added after `componentWillUpdate`? Thanks.\n"}, {"url": "https://api.github.com/repos/airbnb/